@article{0026-1394-55-1-L13,
  author={D B Newell and F Cabiati and J Fischer and others},
  title={The CODATA 2017 values of h , e , k , and N A for the revision of the SI},
  journal={Metrologia},
  volume={55},
  number={1},
  pages={L13},
  url={http://stacks.iop.org/0026-1394/55/i=1/a=L13},
  year={2018},
  abstract={Sufficient progress towards redefining the International System of Units (SI) in terms of exact values of fundamental constants has been achieved. Exact values of the Planck constant h , elementary charge e , Boltzmann constant k , and Avogadro constant N A from the CODATA 2017 Special Adjustment of the Fundamental Constants are presented here. These values are recommended to the 26th General Conference on Weights and Measures to form the foundation of the revised SI.}
}

